    <match lemma="a" tags="pr"><select lemma="a" tags="pr"/></match>
  </rule>
  <rule weight="1.2">
    <match lemma="donar" tags="vblex.*"/>
    <repeat from="0" upto="5">
      <match tags="adv"/>
      <match tags="det.*"/>
      <match tags="adj.*"/>
      <match tags="n.*"/>
    </repeat>
    <match lemma="a" tags="pr"><select lemma="a" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <or><match lemma="ser"/><match lemma="quedar"/></or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="a" tags="pr"><select lemma="em" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="em"/></match>
    <match tags="det.def.*"/>
    <or>
      <seq n="paisos"/>
      <match lemma="regió" tags="n.*"/>
      <match lemma="departament" tags="n.*"/>
      <match lemma="província" tags="n.*"/>
      <match lemma="república" tags="n.*"/>
      <match lemma="península" tags="n.*"/>
      <match lemma="oceà" tags="n.*"/>
      <match lemma="mar" tags="n.*"/>
      <match lemma="muntanya" tags="n.*"/>
      <match lemma="bosc" tags="n.*"/>
      <match lemma="platja" tags="n.*"/>
      <match lemma="Mediterrani" tags="np.*"/>
      <match lemma="Mediterrània" tags="np.*"/>
      <match lemma="Adriàtic" tags="np.*"/>
      <match lemma="Adriàtica" tags="np.*"/>
      <match lemma="Tirrè" tags="np.*"/>
      <match lemma="Tirrena" tags="np.*"/>
      <match lemma="Egeu" tags="np.*"/>
      <match lemma="Egea" tags="np.*"/>
      <match lemma="Atlàntic" tags="np.*"/>
      <match lemma="Pacífic" tags="np.*"/>
    </or>
  </rule>
  <rule weight="1.2">
    <or>
      <seq n="verbs_moviment"/>
      <match lemma="proximitat" tags="n.*"/>
    </or>
    <repeat from="0" upto="3">
      <or>
        <match tags="adv"/>
        <match tags="prn.enc.*"/>
      </or>
    </repeat>
    <or>
      <match lemma="a" tags="pr"><select lemma="a"/></match>
      <match lemma="de" tags="pr"/>
    </or>
    <match tags="det.def.*"/>
    <or>
      <match lemma="península" tags="n.*"/>
      <match lemma="oceà" tags="n.*"/>
      <match lemma="mar" tags="n.*"/>
      <match lemma="muntanya" tags="n.*"/>
      <match lemma="bosc" tags="n.*"/>
      <match lemma="platja" tags="n.*"/>
      <match lemma="Mediterrani" tags="np.*"/>
      <match lemma="Mediterrània" tags="np.*"/>
      <match lemma="Adriàtic" tags="np.*"/>
      <match lemma="Adriàtica" tags="np.*"/>
      <match lemma="Tirrè" tags="np.*"/>
      <match lemma="Tirrena" tags="np.*"/>
      <match lemma="Egeu" tags="np.*"/>
      <match lemma="Egea" tags="np.*"/>
      <match lemma="Atlàntic" tags="np.*"/>
      <match lemma="Pacífic" tags="np.*"/>
    </or>
  </rule>

  <!-- cap a > para, por volta de -->
  <rule weight="0.6">
    <match lemma="cap a" tags="pr"><select lemma="para"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="cap a" tags="pr"><select lemma="por volta de"/></match>
    <repeat from="0" upto="1">
      <match tags="det.def.*"/>
    </repeat>
    <or><match tags="num.*"/><match tags="num"/></or>
  </rule>

  <!-- en > em, de -->
  <!-- amb > com, de -->
  <rule weight="0.6">
    <match lemma="en" tags="pr"><select lemma="em"/></match>
  </rule>
  <rule weight="0.6">
    <match lemma="amb" tags="pr"><select lemma="com"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="viatge" tags="n.*"/>
    <repeat from="0" upto="2">
      <or>
        <match tags="preadv"/>
        <match tags="adv"/>
        <match tags="adj.*"/>
      </or>
    </repeat>
    <or>
      <match lemma="en" tags="pr"><select lemma="de"/></match>
      <match lemma="amb" tags="pr"><select lemma="de"/></match>
    </or>
    <or>
      <seq n="vehicle"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="anar" tags="vblex.*"/>
      <match lemma="tornar" tags="vblex.*"/>
      <match lemma="arribar" tags="vblex.*"/>
      <match lemma="viatjar" tags="vblex.*"/>
      <match lemma="traslladar" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="prn.enc.*"/>
      </or>
    </repeat>
    <or>
      <match lemma="en" tags="pr"><select lemma="de"/></match>
      <match lemma="amb" tags="pr"><select lemma="de"/></match>
    </or>
    <or>
      <seq n="vehicle"/>
    </or>
  </rule>


<!-- **************** ADV **************** -->

  <!-- just > mesmo, logo -->
  <rule weight="0.6">
    <match lemma="just" tags="adv"><select lemma="mesmo"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="just" tags="adv"><select lemma="logo"/></match>
    <match tags="pr"/>
  </rule>
  <rule weight="1.0">
    <match lemma="just" tags="adv"><select lemma="logo"/></match>
    <match tags="adv"/>
  </rule>

  <!-- com > como, quanto -->
  <rule weight="0.6">
    <match lemma="com" tags="adv"><select lemma="como"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="tant"/>
    <match lemma="com" tags="adv"><select lemma="quanto"/></match>
  </rule>

<!-- **************** PRN **************** -->

  <!-- quin > que, qual -->
  <rule weight="0.6">
    <match lemma="quin" tags="prn.itg.*"><select lemma="qual"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="quin" tags="prn.itg.*"><select lemma="que"/></match>
    <or>
      <match tags="n.*"/>
      <match tags="adj.*"/>
    </or>
  </rule>

<!-- **************** REL **************** -->

  <!-- del qual > de que, de quem, cujo -->
  <rule weight="0.6">
    <match lemma="del qual" tags="rel.*"><select lemma="de que"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="det.def.*"/>
    <match tags="n.*"/>
    <match lemma="del qual" tags="rel.*"><select lemma="cujo"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="det.def.*"/>
    <match tags="adj.*"/>
    <match tags="n.*"/>
    <match lemma="del qual" tags="rel.*"><select lemma="cujo"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="det.def.*"/>
    <match tags="n.*"/>
    <match tags="adj.*"/>
    <match lemma="del qual" tags="rel.*"><select lemma="cujo"/></match>
  </rule>

