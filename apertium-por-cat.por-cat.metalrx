<rules>

<!-- **************** PR **************** -->

  <!-- além de > més enllà de, a més de -->
  <rule weight="0.6">
    <match lemma="além de" tags="pr"><select lemma="més enllà de"/></match>
  </rule>

  <!-- para > per a, cap a, a, a punt de -->
  <rule weight="0.6">
    <match lemma="para" tags="pr"><select lemma="per a"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="mudar" tags="vblex.*"><select lemma="traslladar"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="para" tags="pr"><select lemma="a"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="traduzir" tags="vblex.*"/>
      <match lemma="viajar" tags="vblex.*"/>
      <match lemma="fugir" tags="vblex.*"/>
      <!--       <match lemma="ir" tags="vblex.*"/> -->
    </or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="para" tags="pr"><select lemma="a"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="avançar" tags="vblex.*"/>
      <match lemma="xxxviajar" tags="vblex.*"/>
      <!--       <match lemma="ir" tags="vblex.*"/> -->
    </or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="para" tags="pr"><select lemma="cap a"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="estar" tags="vblex.*"/>
    <match lemma="para" tags="pr"><select lemma="a punt de"/></match>
    <repeat from="0" upto="2">
      <match tags="prn.pro.*"/>  
    </repeat>
    <or>
      <match tags="vblex.inf"/>  
      <match tags="vbmod.inf"/>
      <match tags="vbser.inf"/> 
    </or>  
  </rule>

  <!-- até > fins a/fins ... -->
  <rule weight="0.6">
    <match lemma="até" tags="pr"><select lemma="fins a"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="até" tags="pr"><select lemma="fins"/></match>
    <or>
      <match tags="adv"/>
      <match tags="pr"/>
      <match tags="rel.adv"/>
      <match lemma="que"/>
      <match lemma="quando"/>
      <match lemma="quanto"/>
    </or>   
  </rule>
  <rule weight="1.0">
    <match lemma="até" tags="pr"><select lemma="fins a"/></match>
    <or>
      <match tags="vblex.inf"/>  
      <match tags="vbmod.inf"/>
      <match tags="vbser.inf"/> 
    </or>  
  </rule>
  <rule weight="0.8">
    <match lemma="até" tags="pr"><select lemma="fins"/></match>
    <or>
      <match tags="prn.*"/>
      <match tags="vblex.*"/>  
      <match tags="vbmod.*"/>
      <match tags="vbser.*"/> 
    </or>  
  </rule>

  <!-- em > en, a -->
  <rule weight="0.6">
    <match lemma="em" tags="pr"><select lemma="en"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="em" tags="pr"><select lemma="a" tags="pr"/></match>
    <repeat from="0" upto="1">
      <match tags="det.*"/>
    </repeat>
    <or><match tags="np.loc"/><match tags="np.top.*"/><match tags="np.hyd.*"/><match tags="np.al"/><match tags="np.al.*"/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="em" tags="pr"><select lemma="a" tags="pr"/></match>
    <repeat from="0" upto="1">
      <match tags="det.*"/>
    </repeat>
    <or>
      <seq n="lugar_concreto"/>
      <match lemma="são"/>	<!-- endevino que és un topònim -->
      <match lemma="internet" tags="n.*"/>
      <match lemma="Internet" tags="np.al.*"/>
    </or>
  </rule>

  <!-- de > de, en -->
  <rule weight="0.6">
    <match lemma="de" tags="pr"><select lemma="de"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="ir" tags="vblex.*"/>
      <match lemma="vir" tags="vblex.*"/>
      <match lemma="chegar" tags="vblex.*"/>
      <match lemma="viajar" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="prn.enc.*"/>
      </or>
    </repeat>
    <match lemma="de" tags="pr"><select lemma="en"/></match>
    <or>
      <seq n="veículo"/>
    </or>
  </rule>

<!-- **************** REL **************** -->

  <!-- que > que, què -->
  <rule weight="0.6">
    <match lemma="que" tags="rel.*"><select lemma="que"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="pr"/>
    <match lemma="que" tags="rel.*"><select lemma="què"/></match>
  </rule>

<!-- **************** ADJ **************** -->

  <!-- vermelho > vermell, roig -->
  <rule weight="0.6">
    <match lemma="vermelho" tags="adj.*"><select lemma="vermell"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="cruz" tags="n.*"/>
      <match lemma="lua" tags="n.*"/>
      <match lemma="socorro" tags="n.*"/>
      <match lemma="exército" tags="n.*"/>
    </or>
    <match lemma="vermelho" tags="adj.*"><select lemma="roig"/></match>
  </rule>

  <!-- sírio > sirià, siri -->
  <rule weight="0.6">
    <match lemma="sírio" tags="adj.*"><select lemma="sirià"/></match>
  </rule>

  <!-- reformado > reformat, jubilat, retirat -->
  <rule weight="0.6">
    <match lemma="reformado" tags="adj.*"><select lemma="reformat"/></match>
  </rule>
  <rule weight="1.0">
