      <match lemma="episodio" tags="n.*"/>
      <match lemma="história" tags="n.*"/>
      <match lemma="lenda" tags="n.*"/>
      <match lemma="narración" tags="n.*"/>
      <match lemma="narrador" tags="n.*"/>
      <match lemma="parábola" tags="n.*"/>
      <match lemma="filme" tags="n.*"/>
      <match lemma="peripecia" tags="n.*"/>
      <match lemma="relato" tags="n.*"/>
      <match lemma="vida" tags="n.*"/>
    </or>
    <repeat from="0" upto="4">
      <or>
        <match tags="*.*"/>
        <match tags="pr"/>
      </or>
    </repeat> 
    <match lemma="contar" tags="vblex.*"><select lemma="contar"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="contar" tags="vblex.*"><select lemma="contar"/></match>
    <repeat from="0" upto="2">
      <match tags="adv.*"/>
    </repeat>
      <match lemma="que"/>
  </rule>

  <rule weight="1.0">
    <match lemma="contar" tags="vblex.*"><select lemma="comptar"/></match>
    <repeat from="0" upto="2">
      <match tags="adv.*"/>
    </repeat>
    <or>
      <match lemma="com"/>
      <match lemma="comigo"/>
      <match lemma="contigo"/>
      <match lemma="consigo"/>
    </or>
  </rule>
 
  <rule weight="0.9">
    <match lemma="contar" tags="vblex.*"><select lemma="explicar"/></match>
    <match tags="prn.*"/>
  </rule>

  <!-- chamar > cridar, dir, anomenar, qualificar -->

  <macro name="verb_nopron_pron" p1="chamar" p2="cridar" p3="dir"/>

  <rule weight="1.0">
    <match lemma="chamar" tags="vblex.pp.*"><select lemma="anomenar"/></match>
  </rule>

  <rule weight="4.0">
    <match lemma="chamar" tags="vblex.*"><select lemma="cridar"/></match> 
    <repeat from="0" upto="4">
      <or>
        <match tags="*.*"/>
        <match tags="adv"/>
        <match tags="pr"/>
      </or>
    </repeat>
    <match lemma="atenção"/>
  </rule>

  <rule weight="3.0">
    <match tags="vbser.*"/>
    <match lemma="chamar" tags="vblex.pp.*"><select lemma="cridar"/></match>
    <or>
      <match lemma="por" tags="pr"/>
      <match lemma="a" tags="pr"/>
    </or>
  </rule>

  <rule weight="3.0">  
    <match lemma="chamar" tags="vblex.*"><select lemma="cridar"/></match>
    <repeat from="0" upto="10">
      <or>
        <match tags="pr" lemma="a"/>
        <match tags="pr" lemma="de"/>
        <match tags="n.*"/>
        <match tags="det.*"/>
        <match tags="adj.*"/>
        <!--<match tags="pr"/>-->
        <match tags="cnjcoo"/>
      </or>
    </repeat>
    <match lemma="a" tags="pr"/>
    <match tags="*.inf"/>
  </rule>

  <rule weight="3.0">
    <match tags="adv"/>
    <match lemma="chamar" tags="vblex.pp.*"><select lemma="cridar"/></match> 
    <match lemma="por"/>
  </rule>

  <rule weight="1.1">
    <match lemma="chamar" tags="vblex.*"><select lemma="dir"/></match> 
    <match lemma="assim"/>
  </rule>

  <rule weight="3.0">
    <match lemma="chamar" tags="vblex.*"><select lemma="anomenar"/></match> 
    <match tags="prn.enc.*"/>
    <or>
      <match tags="n.*"/>
      <match tags="adj.*"/>
    </or>
  </rule>

  <rule weight="3.0">
    <match lemma="chamar" tags="vblex.*"><select lemma="anomenar"/></match> 
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="de" tags="pr"/>
    <or>
      <match tags="n.*"/>
      <match tags="adj.*"/>
    </or>
  </rule>

  <rule weight="3.0">
    <or>
      <match tags="n.*"/>
      <match tags="adj.*"/>
    </or>
    <match lemma="chamar" tags="vblex.pp.*"><select lemma="anomenar"/></match> 
  </rule>

  <rule weight="5.0">
    <match lemma="como"/>
    <repeat from="0" upto="1">
      <or>
        <match tags="prn.pro.*"/>
        <match tags="vbser.*"/>
      </or>
    </repeat>
    <match lemma="chamar" tags="vblex.*"><select lemma="anomenar"/></match>
  </rule>

  <rule weight="2.0">
    <match tags="rel.*"/>
    <match lemma="chamar" tags="vblex.*"><select lemma="anomenar"/></match>
  </rule>

  <rule weight="4.0">
    <match lemma="chamar" tags="vblex.*"><select lemma="qualificar"/></match>
    <match lemma="de" tags="pr"/>
  </rule>

  <!-- indicar > indicar, nomenar -->
  <rule weight="0.6">
    <match lemma="indicar" tags="vblex.*"><select lemma="indicar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="indicar" tags="vblex.*"><select lemma="nomenar"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <or>
      <match tags="np.ant.*"/>
      <match tags="np.cog.*"/>
    </or>
  </rule>

  <!-- estar > estar, ser -->
  <rule weight="0.6">
    <match lemma="estar" tags="vblex.*"><select lemma="qualificar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="estar" tags="vblex.*"><select lemma="ser"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="em" tags="pr"/>
    <match tags="np.top.*"/>
  </rule>

<!-- **************** PRN **************** -->

  <!-- outro > altre, altre -->
  <rule weight="0.6">
    <match lemma="outro" tags="prn.tn.*.sg"><select lemma="un altre"/></match>
  </rule>
  <rule weight="0.6">
    <match lemma="outro" tags="prn.tn.*.pl"><select lemma="altre"/></match>
  </rule>

  <!-- que > què, quin -->
  <rule weight="0.6">
    <match lemma="que" tags="prn.itg.*"><select lemma="què"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="que" tags="prn.itg.*"><select lemma="quin"/></match>
    <or>
      <match tags="n.*"/>
      <match tags="adj.*"/>
    </or>
  </rule>

